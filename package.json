{
  "name": "amenity-platform",
  "version": "1.0.0",
  "description": "The Ultimate Creator Economy Platform - Combining Meta + YouTube + TikTok + Patreon with 80-90% revenue share",
  "main": "index.js",
  "bin": {
    "amenity": "./bin/amenity.js",
    "amenity-start": "./bin/start.js"
  },
  "keywords": [
    "creator-economy",
    "social-media",
    "streaming",
    "monetization",
    "mobile-app",
    "next-js",
    "expo",
    "video-platform",
    "live-streaming",
    "ai-assistant"
  ],
  "author": "Amenity Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amenity-platform/amenity.git"
  },
  "homepage": "https://amenity-platform.com",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "mobile": "expo start",
    "mobile:android": "expo start --android",
    "mobile:ios": "expo start --ios",
    "mobile:build": "eas build",
    "install-all": "npm install && cd mobile && npm install",
    "build-all": "npm run build && cd mobile && expo export",
    "prepublishOnly": "npm run build-all"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@stripe/stripe-js": "^2.1.0",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^10.16.0",
    "hls.js": "^1.4.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "node-media-server": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.13.0",
    "react-webcam": "^7.1.0",
    "sharp": "^0.32.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.0",
    "stripe": "^14.0.0",
    "tailwindcss": "^3.3.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.14.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^20.8.0",
    "@types/node-media-server": "^2.3.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "typescript": "^5.2.0"
  },
  "expo": {
    "name": "Amenity",
    "slug": "amenity-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amenityapp.ios",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.amenityapp.android",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
