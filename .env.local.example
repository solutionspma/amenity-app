# Supabase Configuration
# Copy this file to .env.local and update with your actual Supabase credentials

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Your Supabase anon key (public key)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Database Configuration (for schema deployment)
SUPABASE_DB_URL=postgresql://postgres:password@localhost:5432/amenity_db

# Optional: Service Role Key (for admin operations)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Auth Configuration
NEXTAUTH_SECRET=your-nextauth-secret-here
NEXTAUTH_URL=http://localhost:3000

# Payment Configuration (Stripe)
STRIPE_PUBLIC_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# AI Configuration (OpenAI for Jay-I Assistant)
OPENAI_API_KEY=sk-...